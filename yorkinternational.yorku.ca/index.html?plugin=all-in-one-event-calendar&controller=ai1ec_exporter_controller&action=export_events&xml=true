ers><date-time>2022-06-10T09:00:00</date-time></rdate><rdate><parameters><tzid><text>America/Toronto</text></tzid></parameters><date-time>2022-07-15T09:00:00</date-time></rdate><exdate><parameters><tzid><text>America/Toronto</text></tzid></parameters><date-time>2021-10-15T09:00:00</date-time></exdate><exdate><parameters><tzid><text>America/Toronto</text></tzid></parameters><date-time>2022-03-17T09:00:00</date-time></exdate><exdate><parameters><tzid><text>America/Toronto</text></tzid></parameters><date-time>2022-03-18T09:00:00</date-time></exdate><exdate><parameters><tzid><text>America/Toronto</text></tzid></parameters><date-time>2022-06-10T09:00:00</date-time></exdate><exdate><parameters><tzid><text>America/Toronto</text></tzid></parameters><date-time>2022-07-15T09:00:00</date-time></exdate><categories><parameters><language><text>en-US</text></language></parameters><text>Go Global Info Sessions</text></categories><contact><text>Marierose Talla; gnl@yorku.ca; https://yorkinternational.yorku.ca/gnl/</text></contact><location><text>Zoom</text></location><sequence><integer>0</integer></sequence><url><uri>https://yorkinternational.yorku.ca/event/gnl-as-a-pedagogy-workshop-cases-and-tools-external/</uri></url><x-cost-type><unknown>free</unknown></x-cost-type><x-tags><parameters><language><text>en-US</text></language></parameters><unknown>Global Learning,Global Learning Programs,GNL,International</unknown></x-tags><x-cost><unknown>50.00</unknown></x-cost></properties></vevent><vevent><properties><uid><text>ai1ec-38450@yorkinternational.yorku.ca</text></uid><dtstamp><date-time>2023-11-13T16:59:09Z</date-time></dtstamp><summary><text>Globally Networked Learning: YorkU Faculty Ask-Aways</text></summary><description><text>&lt;h4&gt;&lt;img decoding="async" loading="lazy" class="alignnone wp-image-37987 size-full" src="https://yorkinternational.yorku.ca/files/2021/08/GNL_Banner5.png" alt="" width="2048" height="622" srcset="https://yorkinternational.yorku.ca/files/2021/08/GNL_B