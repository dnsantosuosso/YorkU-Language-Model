jQuery(document).ready(function() {
jQuery("#edit-undergraduateselect").change(function(){
var temp = jQuery("#edit-undergraduateselect").val();
url = "https://sfs.yorku.ca/fees/courses/2019-2020/fall-winter?url=" + temp;
window.location.replace(url);
});

